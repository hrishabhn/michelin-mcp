{
    "tasks": {
        "parse-sqlite": "deno run --allow-read --allow-write src/scripts/parse-sqlite.ts",
        "parse-neon": "deno run --allow-env --env-file --allow-read --allow-net src/scripts/parse-neon.ts",
        "dev": "deno run --allow-env --env-file --allow-read --allow-net --watch src/main.ts",
        "start": "deno run --allow-env --env-file --allow-read --allow-net src/main.ts"
    },
    "fmt": {
        "useTabs": false,
        "lineWidth": 9999,
        "indentWidth": 4,
        "semiColons": false,
        "singleQuote": true
    }
}
